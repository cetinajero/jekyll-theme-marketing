<a class="{{ include.class }}" href="{{ '/' | relative_url }}" data-component="logos/progressive">
  <img src="{{ site.amazon-s3 }}/components/logos/progressive/img/{{ site.domain | replace:'.',' ' | truncatewords: 1,"" }}.png"
       class="logo-img my-2 my-sm-0"
       alt="{{ site.title }}"
       data-last-deploy="{{ site.time }}"/>
</a>
